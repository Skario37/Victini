      "language": {
        "name": "en",
        "url": "json/language/0009.json"
      },
      "version": {
        "name": "sapphire",
        "url": "json/version/0008.json"
      }
    },
    {
      "flavor_text": "GRAVELER grows by feeding on rocks.\nApparently, it prefers to eat rocks\nthat are covered in moss.\fThis POKéMON eats its way through\na ton of rocks on a daily basis.",
      "language": {
