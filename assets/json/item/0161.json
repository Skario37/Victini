  "flavor_text_entries": [
    {
      "language": {
        "name": "zh-Hans",
        "url": "json/language/0012.json"
      },
      "text": "让宝可梦携带后，\n在受到效果绝佳的火属性招式\n攻击时，能令其威力减弱。",
      "version_group": {
