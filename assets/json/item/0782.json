    {
      "language": {
        "name": "en",
        "url": "json/language/0009.json"
      },
      "text": "A suit made with the collective technological\nknow-how of Team Magma. It can withstand\nany impact.",
      "version_group": {
        "name": "ultra-sun-ultra-moon",
        "url": "json/version-group/0018.json"
      }
    },
    {
      "language": {
        "name": "it",
