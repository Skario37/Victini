      "version_group": {
        "name": "ultra-sun-ultra-moon",
        "url": "json/version-group/0018.json"
      }
    },
    {
      "flavor_text": "Protege al Pokémon de las tormentas de arena, el\ngranizo y los movimientos con polvos.",
