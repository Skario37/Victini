  "effect_entries": [
    {
      "effect": "Inflicts typeless regular damage.  Every Pokémon in the user's party, excepting those that have fainted or have a major status effect, attacks the target.  Calculated stats are ignored; the base stats for the target and assorted attackers are used instead.  The random factor in the damage formula is not used.  dark Pokémon still get STAB.\n\nThis effect breaks through wonder guard.",
      "language": {
        "name": "en",
        "url": "json/language/0009.json"
      },
      "short_effect": "Hits once for every conscious Pokémon the trainer has."
    }
  ],
