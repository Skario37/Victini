      "language": {
        "name": "ja",
        "url": "json/language/0011.json"
      },
      "version_group": {
        "name": "ultra-sun-ultra-moon",
        "url": "json/version-group/0018.json"
      }
    },
    {
      "flavor_text": "On the second turn, an ultracold, freezing wind\nsurrounds the target. This may leave the target with\na burn.",
      "language": {
